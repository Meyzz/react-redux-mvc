{
  "name": "react-redux-mvc",
  "version": "0.8.1",
  "description": "Implementation of MVC pattern based on React-Redux bunch",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run test",
    "example": "better-npm-run example",
    "build": "rm -rf lib && better-npm-run build",
    "lint": "tslint --project tsconfig.json"
  },
  "betterScripts": {
    "example": {
      "command": "webpack --watch --config example.config.js ",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "build": {
      "command": "webpack --color --display-error-details --display-modules --mode production",
      "env": {
        "NODE_ENV": "dev"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/welljs/react-redux-mvc.git"
  },
  "keywords": [
    "React",
    "redux",
    "MVC"
  ],
  "author": "Vitalii Vorobioff <vitalii.vorobioff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/welljs/react-redux-mvc/issues"
  },
  "homepage": "https://github.com/welljs/react-redux-mvc#readme",
  "devDependencies": {
    "@types/lodash": "4.14.116",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "@types/react-redux": "6.0.9",
    "awesome-typescript-loader": "5.2.1",
    "babel-cli": "6.26.0",
    "babel-loader": "8.0.4",
    "better-npm-run": "0.1.1",
    "easy-redux": "0.4.4",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "resolve-url-loader": "3.0.0",
    "source-map-loader": "0.2.4",
    "ts-jest": "23.10.3",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0",
    "tslint-react": "3.6.0",
    "typescript": "3.0.3",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.1",
    "webpack-dev-server": "3.1.8"
  },
  "dependencies": {
    "@types/redux-actions": "2.3.0",
    "hoist-non-react-statics": "3.0.1",
    "lodash": "4.17.11",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-redux": "5.0.7",
    "redux": "4.0.0"
  }
}
