{
  "name": "react-redux-mvc",
  "version": "0.8.1",
  "description": "Implementation of MVC pattern based on React-Redux bunch",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run test",
    "example": "better-npm-run example",
    "build": "babel src --out-dir lib",
    "dev": "better-npm-run dev"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack --watch",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "example": {
      "command": "webpack --watch --config example.config.js ",
      "env": {
        "NODE_ENV": "dev"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/welljs/react-redux-mvc.git"
  },
  "keywords": [
    "React",
    "redux",
    "MVC"
  ],
  "author": "Vitalii Vorobioff <vitalii.vorobioff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/welljs/react-redux-mvc/issues"
  },
  "homepage": "https://github.com/welljs/react-redux-mvc#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.74",
    "awesome-typescript-loader": "^3.1.3",
    "better-npm-run": "0.0.10",
    "html-webpack-plugin": "^2.28.0",
    "resolve-url-loader": "^2.0.2",
    "source-map-loader": "^0.2.1",
    "tslint": "^5.4.3",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typescript": "^2.5.0",
    "webpack": "^2.1.0-beta.6",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "@types/react": "^15.0.27",
    "@types/react-dom": "^15.5.0",
    "react-dom": "^15.4.0",
    "easy-redux": "^0.4.0",
    "hoist-non-react-statics": "^1.2.0",
    "react": "^15.4.0",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0"
  }
